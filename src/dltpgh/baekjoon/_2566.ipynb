   "source": [
    "max_num = 0\n",
    "x = 0\n",
    "y = 0\n",
    "\n",
    "for i in range(9):\n",
    "    a = list(map(int, input().split()))\n",
    "    if max_num < max(a):\n",
    "        max_num = max(a)\n",
    "        x = i\n",
    "        y = a.index(max_num)\n",
    "\n",
    "print(max_num)\n",
    "print(x+1, y+1)"
   ]
